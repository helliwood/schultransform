{
  "name": "trollfjord/content-tree-bundle",
  "description": "Trollfjord ContentTreeBundle",
  "license": "proprietary",
  "keywords": [
    "trollfjord",
    "content-tree"
  ],
  "homepage": "https://helliwood.de",
  "authors": [
    {
      "name": "Maurice Karg",
      "email": "karg@helliwood.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-dom": "*"
  },
  "require-dev": {
    "slevomat/coding-standard": "^5.0",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "scripts": {
    "cs-check": "phpcs -s -n --runtime-set ignore_warnings_on_exit true .",
    "cs-fix": "phpcbf"
  },
  "autoload": {
    "psr-4": {
      "Trollfjord\\Bundle\\ContentTreeBundle\\": "."
    }
  }
}
