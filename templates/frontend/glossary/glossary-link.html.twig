{% if data|default(null) %}<glossary-link
            word-displayed="{{ data.wordDisplayed }}"
            is-related-word="{{ data.isRelatedWord }}"
            word="{{ data.word }}"
            word-id="{{ data.wordId }}"
            slug="{{ data.slug }}"
            image="{{ data.image }}"
            href="{{ data.href }}"
    ></glossary-link>{% endif %}